    'bundles/leaphlycontent/js/flatui-radio.js'
    'bundles/leaphlycontent/js/jquery.tagsinput.js'
    'bundles/leaphlycontent/js/jquery.placeholder.js'
    'bundles/leaphlycontent/js/application.js'
    'bundles/leaphlycontent/js/highlight.js/highlight.pack.js'
    '@FOSJsRoutingBundle/Resources/public/js/router.js'
    'js/fos_js_routes.js'
    '@LeaphlyContentBundle/Resources/public/js/handlebars.js'
    '@LeaphlyContentBundle/Resources/public/js/cart.js'
